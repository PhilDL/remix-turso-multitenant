{
  "name": "remix-turso-multitenant",
  "version": "1.5.2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm run clean && node ./server.mjs",
    "build": "npm run clean && remix vite:build",
    "build:icons": "tsx ./scripts/build-icons.ts",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true node server.mjs",
    "test": "vitest",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "clean": "rimraf build",
    "nuke": "npm run clean && rimraf node_modules package-lock.json && npm i",
    "act": "doppler run -- act -s FLY_API_TOKEN --container-architecture linux/amd64",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "drizzle:generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./drizzle/schema.ts",
    "drizzle:generate:tenants": "drizzle-kit generate:sqlite --out ./drizzle/migrations-tenants --breakpoints --schema=./drizzle/tenant-schema.ts",
    "drizzle:migrate": "cross-env tsx drizzle/migrate",
    "drizzle:migrate:tenant": "cross-env tsx drizzle/migrate-tenant"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build"
  ],
  "dependencies": {
    "@conform-to/react": "^1.1.0",
    "@conform-to/zod": "^1.1.0",
    "@libsql/client": "^0.6.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/express": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@t3-oss/env-core": "^0.9.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.8",
    "express": "^4.18.3",
    "isbot": "^5.1.1",
    "ky": "^1.2.3",
    "lucide-react": "^0.368.0",
    "morgan": "^1.10.0",
    "react": "18.3.0-canary-2f8f77602-20240229",
    "react-dom": "18.3.0-canary-2f8f77602-20240229",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "remix-typedjson": "^0.4.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@playwright/test": "^1.42.1",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@remix-run/testing": "^2.8.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.56.5",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "all-contributors-cli": "^6.26.1",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "jsdom": "^24.0.0",
    "node-html-parser": "^6.1.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "remix-development-tools": "^3.7.4",
    "remix-flat-routes": "^0.6.4",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.2",
    "typescript": "5.1.6",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "react": "18.3.0-canary-2f8f77602-20240229",
    "react-dom": "18.3.0-canary-2f8f77602-20240229"
  }
}
