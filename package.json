{
  "name": "remix-turso-multitenant",
  "version": "1.5.2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "pnpm run clean && vite",
    "build": "pnpm run clean && remix vite:build",
    "build:icons": "tsx ./scripts/build-icons.ts",
    "start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true node ./build/server/index.js",
    "test": "vitest",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "coverage": "vitest run --coverage",
    "test:e2e": "pnpm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "pnpm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "test:e2e:ci": "pnpm run test:e2e:install && pnpm run test:e2e:run",
    "clean": "pnpm rimraf build",
    "nuke": "pnpm run clean && pnpm rimraf node_modules package-lock.json && pnpm i",
    "act": "doppler run -- act -s FLY_API_TOKEN --container-architecture linux/amd64",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "drizzle:generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./drizzle/schema.ts",
    "drizzle:generate:tenants": "drizzle-kit generate:sqlite --out ./drizzle/migrations-tenants --breakpoints --schema=./drizzle/tenant-schema.ts",
    "drizzle:migrate": "cross-env tsx drizzle/migrate",
    "drizzle:migrate:tenant": "cross-env tsx drizzle/migrate-tenant",
    "drizzle:studio": "drizzle-kit studio"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build"
  ],
  "dependencies": {
    "@conform-to/react": "^1.1.3",
    "@conform-to/zod": "^1.1.3",
    "@epic-web/remember": "^1.0.2",
    "@lemonsqueezy/lemonsqueezy.js": "^3.0.0",
    "@libsql/client": "^0.6.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@t3-oss/env-core": "^0.10.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "express": "^4.18.3",
    "isbot": "^5.1.6",
    "ky": "^1.2.4",
    "lucide-react": "^0.378.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.5.0",
    "remix-create-express-app": "^0.3.7",
    "remix-utils": "^7.6.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.7",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@playwright/test": "^1.44.0",
    "@remix-run/dev": "^2.9.2",
    "@remix-run/eslint-config": "^2.9.2",
    "@remix-run/testing": "^2.9.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "all-contributors-cli": "^6.26.1",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.21.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "execa": "^9.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.15",
    "jsdom": "^24.0.0",
    "node-html-parser": "^6.1.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "remix-development-tools": "^4.1.6",
    "remix-express-dev-server": "^0.2.5",
    "remix-flat-routes": "^0.6.5",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.10.5",
    "typescript": "5.4.5",
    "vite": "^5.1.0",
    "vite-env-only": "2.4.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "vite-env-only@2.4.0": "patches/vite-env-only@2.4.0.patch"
    }
  }
}
